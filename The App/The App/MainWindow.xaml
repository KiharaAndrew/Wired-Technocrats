<Window x:Class="The_App.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:The_App"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Background="#FF47786D">
            <Button Margin="30" Height="30"/>
            <Button Margin="30" Height="30"/>
            <Button Margin="30" Height="30"/>
            <Button Margin="30" Height="30"/>
            <Button Margin="30" Height="30"/>
        </StackPanel>
        <StackPanel Background="#FF47786D" Grid.Column="1">
            <Button Margin="30" Height="30"/>
            <Button Margin="30" Height="30"/>
            <Button Margin="30" Height="30"/>
            <Button Margin="30" Height="30"/>
            <Button Margin="30" Height="30"/>
        </StackPanel> 
    </Grid>
</Window>
